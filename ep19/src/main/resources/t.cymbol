struct Point {
    int x;
    int y;

    void init(int x_val, int y_val) {
        x = x_val;
        y = y_val;
    }
}

void main() {
    print("Hello, World!");

    Point p;

    // 调用结构体方法初始化
    p.init(10, 20);

    print("Point coordinates:");
    print(p.x);
    print(p.y);
    print(p.x + p.y);
}
