.def main: args=0, locals=2
    ; 初始化 i = 0
    li r1, 0
    sw r1, r14, 0   ; store i at fp+0

    ; 初始化 sum = 0
    li r1, 0
    sw r1, r14, 1   ; store sum at fp+1

loop_start:
    ; 加载 i 到 r1
    lw r1, r14, 0   ; r1 = i

    ; 比较 i >= 10
    li r2, 10       ; r2 = 10
    sge r3, r1, r2  ; r3 = (i >= 10)
    jt r3, loop_end ; if r3 != 0, jump to loop_end

    ; sum += i
    lw r4, r14, 1   ; r4 = sum
    add r4, r4, r1  ; r4 = sum + i
    sw r4, r14, 1   ; store sum

    ; i++
    li r5, 1        ; r5 = 1
    add r1, r1, r5  ; r1 = i + 1
    sw r1, r14, 0   ; store i

    j loop_start

loop_end:
    ; 加载 sum 到 r1 作为返回值
    lw r1, r14, 1
    halt
