struct Point {
    int x;
    int y;

}

int add(int x,int y) {
    int d = x + y;
    if (d >= 0) {
        d = d + 1;
    }
    return d;
}

int fib(int n) {
    if (n < 2) return n;
    else return add(fib(n-1),fib(n-2));
}



float k = fib(10);
