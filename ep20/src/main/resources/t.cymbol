
int fib(int n){
    int p = 1;
    int q = 1;
    while(n >= 0) {
        int tmp = q;
        q = p + q;
        p = tmp;
        n = n - 1;
        if(n >= 3) {
            if (n <= 18) {
                return p*2 + 1;
            }
            return p;
        }
    }
    return n;
}

int main() {

   print(fib(10));
   return 0;
}

