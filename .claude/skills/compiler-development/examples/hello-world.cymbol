// Hello World示例程序
// 展示Cymbol语言的基本结构

int main() {
    // 变量声明和初始化
    int x = 10;
    float y = 3.14;
    bool flag = true;
    string message = "Hello, Cymbol!";

    // 算术运算
    int sum = x + 5;
    float product = x * y;

    // 条件语句
    if (x > 5) {
        print("x is greater than 5");
    } else {
        print("x is 5 or less");
    }

    // 循环语句
    int i = 0;
    while (i < 3) {
        print("Iteration: " + i);
        i = i + 1;
    }

    // 函数调用
    print(message);

    // 返回语句
    return 0;
}