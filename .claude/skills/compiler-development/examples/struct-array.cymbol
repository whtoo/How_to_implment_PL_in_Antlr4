// 结构体和数组示例
// 展示Cymbol语言的高级特性

// 定义结构体类型
struct Point {
    int x;
    int y;
}

// 定义类型别名
typedef int MyInt;

int main() {
    // 结构体变量声明和初始化
    Point p1;
    p1.x = 10;
    p1.y = 20;

    // 数组声明和初始化
    int[5] numbers = {1, 2, 3, 4, 5};
    float[] floats = {1.1, 2.2, 3.3};  // 动态大小数组

    // 结构体数组
    Point[3] points;
    points[0].x = 1;
    points[0].y = 2;
    points[1].x = 3;
    points[1].y = 4;

    // 数组遍历
    int i = 0;
    while (i < 5) {
        print("numbers[" + i + "] = " + numbers[i]);
        i = i + 1;
    }

    // 类型别名使用
    MyInt age = 25;
    print("Age: " + age);

    // 结构体字段访问
    print("Point p1: (" + p1.x + ", " + p1.y + ")");

    // 类型转换示例
    float result = (float)numbers[0] + floats[0];
    print("Type conversion result: " + result);

    return 0;
}